<odoo>
    <record id="ir_cron_sftp_contact_sync" model="ir.cron">
        <field name="name">Synchronisation SFTP des contacts</field>
        <field name="model_id" ref="model_sftp_contact_sync"/>
        <field name="state">code</field>
        <field name="code">model.process_contacts()</field>
        <field name="interval_number">24</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
    </record>
</odoo>