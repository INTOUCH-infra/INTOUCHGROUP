<odoo>
    <record id="view_partner_form_inherit_call_logs" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.call.logs</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <sheet position="inside">
                <group>
                    <field name="call_log_ids">
                        <tree>
                            <field name="call_date"/>
                            <field name="call_duration"/>
                            <field name="call_notes"/>
                        </tree>
                    </field>
                </group>
            </sheet>
        </field>
    </record>
</odoo>
