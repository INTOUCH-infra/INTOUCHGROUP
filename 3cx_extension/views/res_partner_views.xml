<odoo>
    <record id="view_partner_form_inherit_call_logs" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.call.logs</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='internal_notes']" position="after">
                <page name="call_logs" string="Call Logs">
                    <group>
                        <field name="call_log_ids" nolabel="1">
                            <tree>
                                <field name="date"/>
                                <field name="name"/>
                                <field name="ttype"/>
                                <field name="agentname"/>
                                <field name="agent"/>
                                <field name="duration"/>
                                <field name="details"/>
                            </tree>
                        </field>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
